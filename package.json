{
	"name": "inter-view",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "npm run rollup && node ./bin/www",
		"watch": "nodemon ./bin/www",
		"babel": "babel public/js/source/script.js --out-file public/js/script.js",
		"rollup": "rollup public/js/source/script.js --format iife --name 'bundle' --file public/js/bundle.js && minify public/js/bundle.js --out-file public/js/bundle.min.js",
		"dev": "npm run rollup && npm run watch"
	},
	"engines": {
		"node": ">=10"
	},
	"dependencies": {
		"compression": "^1.7.2",
		"cookie-parser": "~1.4.3",
		"debug": "~2.6.9",
		"dotenv": "^6.0.0",
		"ejs": "~2.5.7",
		"express": "~4.16.0",
		"express-session": "^1.15.6",
		"http-errors": "~1.6.2",
		"is-elevated": "^2.0.1",
		"morgan": "~1.9.0",
		"multer": "^1.3.0",
		"mysql": "^2.15.0",
		"node-sass-middleware": "0.11.0",
		"session-file-store": "^1.2.0"
	},
	"nodemonConfig": {
		"ignore": [
			"sessions/*",
			"public/*",
			"design/*"
		]
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-minify": "^0.4.3",
		"babel-preset-env": "^1.7.0",
		"rollup": "^0.60.1"
	},
	"babel": {
		"presets": [
			"env"
		]
	}
}
