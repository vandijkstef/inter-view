{
	"name": "inter-view",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "npm run build && node ./bin/www",
		"watch": "nodemon ./bin/www",
		"build": "babel public/js/source/script.js --out-file public/js/script.js",
		"dev": "npm run build && npm run watch",
		"cert": "openssl req -new -newkey rsa:2048 -nodes -out localhost.csr -keyout private.key",
		"sign": "openssl x509 -req -in localhost.csr -signkey private.key -out server.crt",
		"docert": "npm run cert && npm run sign"
	},
	"engines" : { 
		"node" : ">=10" 
	}, 
	"dependencies": {
		"cookie-parser": "~1.4.3",
		"debug": "~2.6.9",
		"dotenv": "^6.0.0",
		"ejs": "~2.5.7",
		"express": "~4.16.0",
		"express-session": "^1.15.6",
		"http-errors": "~1.6.2",
		"morgan": "~1.9.0",
		"mysql": "^2.15.0",
		"node-sass-middleware": "0.11.0",
		"session-file-store": "^1.2.0"
	},
	"nodemonConfig": {
		"ignore": [
			"sessions/*",
			"public/*"
		]
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-env": "^1.7.0"
	},
	"babel": {
		"presets": [
			"env"
		]
	}
}
